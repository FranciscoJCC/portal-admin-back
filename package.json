{
  "name": "portal-admin-back",
  "version": "1.0.0",
  "description": "Portal de administaci√≥n de usuarios y plataformas",
  "main": "index.js",
  "scripts": {
    "dev": "SET NODE_ENV=dev&& nodemon server.js",
    "prod": "SET NODE_ENV=prod&& nodemon server.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeders:generate": "npx sequelize-cli seed:generate --name",
    "seeders:undo": "npx sequelize-cli db:seed:undo --seed ",
    "seeders:run": "npx sequelize-cli db:seed:all"
  },
  "author": "Francisco Cervantes",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.11.0",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
